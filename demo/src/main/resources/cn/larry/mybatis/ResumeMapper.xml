<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.larry.mybatis.PersonMapper">
    <select id="selectPerson" resultType="cn.larry.Person">
        select * from t_person where name = #{name}
    </select>
    <select id="selectBaseInfo" resultSetType="cn.larry.entity.BaseInfo">
        select * from demoapp_resume_base_info  limit #{start},#{stop}
    </select>
</mapper>